function solution(num_list) {
    var answer = 0;
    num_list.forEach((num) => {
        let temp = num;
        while (temp !== 1) {
            temp = Math.floor(temp / 2);
            answer++;
        }
    });
    return answer;
}

// 문제 : https://school.programmers.co.kr/learn/courses/30/lessons/181880
