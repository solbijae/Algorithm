function solution(n) {
    let answer = 0;
    for (let i=4; i<=n; i++) {
        if (i % 2 === 0 || i % 3 === 0 || (i % 5 === 0 && i !== 5) || (i % 7 === 0 && i !== 7)) {
            answer += 1;
        }
    }

    return answer;
}

// 문제 : https://school.programmers.co.kr/learn/courses/30/lessons/120846
